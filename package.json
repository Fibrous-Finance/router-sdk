{
  "name": "fibrous-router-sdk",
  "version": "0.6.0-rc.1",
  "description": "A Javascript SDK for Fibrous Finance",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "execute": "npx ts-node",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\" --ignore-path .gitignore",
    "build": "rimraf dist && microbundle --tsconfig tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "kermo kermo@fibrous.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@eslint/js": "^9.29.0",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.6.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.34.1"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "ethers": "^6.14.4",
    "starknet": "^6.24.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
