{
  "name": "fibrous-router-sdk",
  "version": "0.6.1",
  "description": "A Javascript SDK for Fibrous Finance",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "execute": "npx ts-node",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\" --ignore-path .gitignore",
    "build": "rimraf dist && microbundle --tsconfig tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "kermo kermo@fibrous.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.7.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "starknet": "^7.6.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
