{
  "name": "fibrous-router-sdk",
  "version": "1.0.1",
  "description": "A Javascript SDK for Fibrous Finance",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.m.js"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "default": "./dist/index.m.js"
  },
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "execute": "npx ts-node",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\" --ignore-path .gitignore",
    "build": "rimraf dist && microbundle --tsconfig tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "version:beta": "pnpm version prerelease --preid beta",
    "publish:beta": "pnpm build && pnpm publish --tag beta --no-git-checks"
  },
  "keywords": [],
  "author": "kermo kermo@fibrous.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.7.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "starknet": "^7.6.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
