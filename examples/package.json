{
  "name": "fibrous-sdk-examples",
  "version": "0.5.1",
  "description": "Examples for using Fibrous SDK",
  "main": "index.js",
  "author": "kermo & zetsuboii",
  "license": "MIT",
  "private": false,
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "base:tokens": "npx tsx src/base/tokens.ts",
    "base:protocols": "npx tsx src/base/protocols.ts",
    "base:route": "npx tsx src/base/route.ts",
    "base:build": "npx tsx src/base/buildTransaction.ts",
    "scroll:tokens": "npx tsx src/scroll/tokens.ts",
    "scroll:protocols": "npx tsx src/scroll/protocols.ts",
    "scroll:route": "npx tsx src/scroll/route.ts",
    "scroll:build": "npx tsx src/scroll/buildTransaction.ts",
    "starknet:tokens": "npx tsx src/starknet/tokens.ts",
    "starknet:protocols": "npx tsx src/starknet/protocols.ts",
    "starknet:route": "npx tsx src/starknet/route.ts",
    "starknet:build": "npx tsx src/starknet/buildTransaction.ts",
    "starknet:batch": "npx tsx src/starknet/batchRoute.ts",
    "starknet:batch-build": "npx tsx src/starknet/buildBatchTransaction.ts",
    "setup:env": "echo 'Please create a .env file with required environment variables. Check README for details.'"
  },
  "dependencies": {
    "fibrous-router-sdk": "^0.5.1",
    "starknet": "^7.6.4",
    "tsx": "^4.20.3"
